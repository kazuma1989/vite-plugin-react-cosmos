{
  "name": "vite-plugin-react-cosmos",
  "version": "0.0.8",
  "keywords": [
    "vite-plugin"
  ],
  "files": [
    "node/index.d.ts",
    "node/index.cjs",
    "client/index.d.ts",
    "client/index.js",
    "client/lib.js",
    "public/*"
  ],
  "types": "./node/index.d.ts",
  "main": "./node/index.cjs",
  "exports": {
    ".": {
      "types": "./node/index.d.ts",
      "require": "./node/index.cjs"
    },
    "./client": {
      "types": "./client/index.d.ts",
      "import": "./client/index.js"
    }
  },
  "scripts": {
    "update-public": "cosmos-export",
    "build": "pnpm run build:js && pnpm run build:types",
    "build:js": "./build.mjs",
    "build:types": "pnpm run build:types:node && pnpm run build:types:client",
    "build:types:client": "tsc --project ./client/tsconfig.json",
    "build:types:node": "tsc --project ./node/tsconfig.json"
  },
  "dependencies": {
    "es-module-lexer": "0.10.5",
    "fast-glob": "3.2.11"
  },
  "devDependencies": {
    "@tsconfig/vite-react": "1.0.0",
    "@types/node": "16.x.x",
    "@types/react": "18.0.12",
    "esbuild": "0.14.43",
    "react-cosmos": "5.7.2",
    "regenerator-runtime": "0.13.9",
    "sirv": "2.0.2",
    "vite": "2.9.10"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "vite": ">=2.9"
  }
}
